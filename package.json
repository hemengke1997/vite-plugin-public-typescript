{
  "name": "vite-plugin-public-typescript",
  "version": "4.2.0",
  "packageManager": "pnpm@9.15.9",
  "description": "Vite plugin for injecting TypeScript into HTML",
  "type": "module",
  "keywords": [
    "vite",
    "typescript",
    "public-typescript",
    "inject script",
    "html"
  ],
  "license": "MIT",
  "homepage": "https://github.com/hemengke1997/vite-plugin-public-typescript",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/vite-plugin-public-typescript.git"
  },
  "author": "hemengke <https://github.com/hemengke1997>",
  "files": [
    "*.d.ts",
    "*.md",
    "dist"
  ],
  "main": "./dist/node/index.cjs",
  "module": "./dist/node/index.js",
  "types": "./dist/node/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/node/index.d.ts",
      "require": "./dist/node/index.cjs",
      "import": "./dist/node/index.js"
    },
    "./virtual": {
      "types": "./virtual.d.ts"
    },
    "./manifest": {
      "types": "./manifest.d.ts"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "rm -rf dist && tsup",
    "prepare": "simple-git-hooks",
    "test": "vitest",
    "commitlint": "commitlint -e",
    "lint": "tsc --noEmit && eslint . --fix",
    "up": "taze -I -w",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "bump": "bumpp package.json -c --no-push -t --all -x \"pnpm run changelog\"",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "peerDependencies": {
    "vite": ">=5"
  },
  "dependencies": {
    "@antfu/install-pkg": "^1.1.0",
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "browserslist": "^4.24.5",
    "browserslist-to-esbuild": "^2.1.1",
    "core-js": "^3.42.0",
    "debug": "^4.4.1",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "local-pkg": "^1.1.1",
    "lodash.omit": "^4.5.0",
    "magic-string": "^0.30.17",
    "on-change": "^5.0.1",
    "parse5": "^7.3.0",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "std-env": "^3.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@minko-fe/commitlint-config": "^2.2.0",
    "@minko-fe/eslint-config": "^4.2.3",
    "@minko-fe/prettier-config": "^2.2.6",
    "@minko-fe/tsconfig": "^2.1.2",
    "@types/babel__core": "^7.20.5",
    "@types/babel__preset-env": "^7.10.0",
    "@types/debug": "^4.1.12",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash.omit": "^4.5.9",
    "@types/node": "^22.15.18",
    "@types/prompts": "^2.4.9",
    "bumpp": "^10.1.1",
    "conventional-changelog-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.4",
    "eslint": "^9.27.0",
    "npm-run-all2": "^8.0.2",
    "simple-git-hooks": "^2.13.0",
    "taze": "^19.1.0",
    "tsup": "8.3.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitepress": "^1.6.3",
    "vitest": "^3.1.3"
  },
  "simple-git-hooks": {
    "commit-msg": "npm run commitlint",
    "pre-commit": "npm run lint"
  },
  "commitlint": {
    "extends": [
      "@minko-fe/commitlint-config"
    ]
  },
  "prettier": "@minko-fe/prettier-config"
}
