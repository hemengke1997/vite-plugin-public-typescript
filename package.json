{
  "name": "vite-plugin-public-typescript",
  "type": "module",
  "version": "1.3.3",
  "description": "A vite plugin that compiles typescript to javascript for direct use in the browser. 编译typescript至javascript，供浏览器直接使用",
  "author": "hemengke <https://github.com/hemengke1997>",
  "license": "MIT",
  "homepage": "https://github.com/hemengke1997/vite-plugin-public-typescript",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/vite-plugin-public-typescript.git"
  },
  "keywords": [
    "vite",
    "public",
    "typescript",
    "public-typescript",
    "vite browser"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "lint": "eslint .",
    "preinstall": "npx only-allow pnpm",
    "up": "pnpm update --i --L",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "bump": "bumpp package.json -c --all -p -t"
  },
  "peerDependencies": {
    "vite": "3 || 4"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "esbuild": "^0.19.2",
    "fs-extra": "^11.1.1",
    "on-change": "^4.0.2",
    "sirv": "^2.0.3",
    "tiny-glob": "^0.2.9",
    "watcher": "^2.3.0"
  },
  "devDependencies": {
    "@minko-fe/eslint-config": "^1.2.33",
    "@minko-fe/tsconfig": "^1.2.33",
    "@types/debug": "^4.1.8",
    "@types/fs-extra": "^11.0.1",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^20.6.0",
    "bumpp": "^9.2.0",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.49.0",
    "mock-fs": "^5.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.4"
  }
}
