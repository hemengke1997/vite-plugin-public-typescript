{
  "name": "vite-plugin-public-typescript",
  "version": "4.1.4",
  "packageManager": "pnpm@9.15.4",
  "description": "Vite plugin for injecting TypeScript into HTML",
  "type": "module",
  "keywords": [
    "vite",
    "typescript",
    "public-typescript",
    "inject script",
    "html"
  ],
  "license": "MIT",
  "homepage": "https://github.com/hemengke1997/vite-plugin-public-typescript",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/vite-plugin-public-typescript.git"
  },
  "author": "hemengke <https://github.com/hemengke1997>",
  "files": [
    "*.d.ts",
    "*.md",
    "dist"
  ],
  "main": "./dist/node/index.cjs",
  "module": "./dist/node/index.js",
  "types": "./dist/node/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/node/index.d.ts",
      "require": "./dist/node/index.cjs",
      "import": "./dist/node/index.js"
    },
    "./virtual": {
      "types": "./virtual.d.ts"
    },
    "./manifest": {
      "types": "./manifest.d.ts"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "rm -rf dist && tsup",
    "prepare": "simple-git-hooks",
    "test": "run-s test:unit test:serve test:build",
    "test:unit": "vitest run",
    "test:serve": "vitest run -c vitest.config.e2e.ts",
    "test:build": "cross-env VITE_TEST_BUILD=1 vitest run -c vitest.config.e2e.ts",
    "commitlint": "commitlint -e",
    "lint": "tsc --noEmit && eslint . --fix",
    "up": "taze -I -w",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "bump": "bumpp package.json -c --no-push -t --all -x \"pnpm run changelog\"",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "peerDependencies": {
    "vite": ">=5"
  },
  "dependencies": {
    "@antfu/install-pkg": "^1.0.0",
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-typescript": "^7.26.0",
    "browserslist": "^4.24.4",
    "browserslist-to-esbuild": "^2.1.1",
    "core-js": "^3.40.0",
    "debug": "^4.4.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "local-pkg": "^1.0.0",
    "lodash.omit": "^4.5.0",
    "magic-string": "^0.30.17",
    "on-change": "^5.0.1",
    "parse5": "^7.2.1",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "std-env": "^3.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@minko-fe/commitlint-config": "^2.1.2",
    "@minko-fe/eslint-config": "^4.1.1",
    "@minko-fe/tsconfig": "^2.1.1",
    "@types/babel__core": "^7.20.5",
    "@types/babel__preset-env": "^7.10.0",
    "@types/debug": "^4.1.12",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash.omit": "^4.5.9",
    "@types/node": "^22.10.10",
    "@types/prompts": "^2.4.9",
    "bumpp": "^9.11.1",
    "conventional-changelog-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.19.0",
    "npm-run-all2": "^7.0.2",
    "simple-git-hooks": "^2.11.1",
    "taze": "^18.3.0",
    "tsup": "8.3.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vitepress": "^1.6.3",
    "vitest": "^2.1.8",
    "vitest-e2e": "^0.0.10"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "simple-git-hooks": {
    "commit-msg": "npm run commitlint",
    "pre-commit": "npm run lint"
  },
  "commitlint": {
    "extends": [
      "@minko-fe/commitlint-config"
    ]
  }
}
